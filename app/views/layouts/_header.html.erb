<!-- BEGIN HEADER -->
<div class="header navbar navbar-inverse "> 
 	<div class="navbar-inner">
		<div class="header-seperation"> 
			
    </div>
   	<div class="header-quick-nav" > 
   		<div class="pull-left">
        <ul class="nav quick-section">
          <li class="quicklinks"> 
          	<% if request.subdomain.present? && request.subdomain != "www" %>
	          	<% if user_signed_in? %>
	          		<%= link_to request.subdomain + ".leasify.co", "/", target: "_blank" %> 
	          	<% end %>
          	<% end %>
          </li>
        </ul>
      </div>
   		<div class="pull-right">
				<ul class="nav quick-section ">
					<li class="quicklinks">
   					<% if user_signed_in? %>
			   			<%= link_to "Dashboard", dashboard_path %>
			   		<% end %>
   				</li>
   				<% if user_signed_in? %>
					<li class="quicklinks"> 
						<a data-toggle="dropdown" class="dropdown-toggle  pull-right" href="#">						
							<div class="iconset top-down-arrow" style="float:right; margin-top:7px; margin-left:5px;">
							</div>
							<div class="iconset top-settings-dark pull-right" style="margin-left:20px;">
							</div> 	
						</a>
						<ul class="dropdown-menu  pull-right" role="menu" aria-labelledby="dropdownMenu">
							<li><%= link_to "My Dashboard", root_url(:subdomain => false) + "dashboard" %></li>
							<li><%= link_to "Settings", "#" %></li>
							
							<li class="divider"></li>                
							<li><%= link_to "Logout", logout_path %></li>
						</ul>
					</li> 
					<% end %>
				</ul>
   		</div>
   	</div> 
  </div>
</div>
<!-- END HEADER -->